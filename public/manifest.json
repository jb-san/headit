{
  "manifest_version": 2,

  "name": "Headit",
  "description": "This is an example plugin",
  "version": "0.1.0",

  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Edit Headers",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "incognito": "spanning",
  "permissions": [
    "contextMenus",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
